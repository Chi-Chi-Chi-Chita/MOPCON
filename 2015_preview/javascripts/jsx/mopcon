var ReactCSSTransitionGroup=React.addons.CSSTransitionGroup,MopconIntro=React.createClass({displayName:"MopconIntro",getInitialState:function(){return{data:[],dataTemp:{},photoCounts:10,photoChangeTimer:1e4,currentCount:0}},changePhoto:function(){var t=this.state.data;tempPhoto=t[0],t.push(tempPhoto),this.setState({data:t}),setTimeout(function(){t.splice(0,1),this.setState({data:t})}.bind(this),600)},componentDidMount:function(){photos=[];var t="https://api.flickr.com/services/rest/?method=flickr.groups.pools.getPhotos&api_key=e8a0be0224faddd8b45f1170c0e555eb&group_id=2768675%40N23&per_page=500&page=2&format=json&nojsoncallback=1";$.getJSON(t,{format:"json"}).done(function(t){perpagePhoto=t.photos.photo.length,photoCounts=this.state.photoCounts,imgSize="z";for(var o=photoCounts-1;o>=0;o--)photosNum=Math.floor(Math.random()*perpagePhoto),url="https://farm"+t.photos.photo[photosNum].farm+".staticflickr.com/"+t.photos.photo[photosNum].server+"/"+t.photos.photo[photosNum].id+"_"+t.photos.photo[photosNum].secret+"_"+imgSize+".jpg",photo={url:url},photos.push(photo);this.setState({dataTemp:t,data:photos})}.bind(this)),setInterval(function(){this.changePhoto()}.bind(this),this.state.photoChangeTimer)},render:function(){var t=this.state.data.map(function(t,o){return bgstyle={backgroundImage:"url("+t.url+")"},React.createElement("li",{style:bgstyle,ref:"photo_"+o,className:"list"})}.bind(this));return React.createElement("ul",{className:"list-bg"},React.createElement(ReactCSSTransitionGroup,{transitionName:"reactAnimation"},t))}});document.getElementById("mopcon")&&React.render(React.createElement(MopconIntro,null),document.getElementById("mopcon"));